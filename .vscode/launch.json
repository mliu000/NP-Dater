{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug test.exe",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/backend/cpp/cpp_runner/test.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/backend/cpp",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Pretty print STL containers",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}